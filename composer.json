{
  "name": "contributte/application",
  "description": "Extra contrib to nette/application",
  "keywords": [
    "nette",
    "application",
    "presenter",
    "component",
    "control"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/application",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/application": "^4.0.0"
  },
  "require-dev": {
    "nette/http": "^4.0.0",
    "contributte/qa": "^0.4.0",
    "contributte/phpstan": "^0.1.0",
    "contributte/tester": "^0.3.0",
    "psr/http-message": "~1.0.1",
    "tracy/tracy": "~2.9.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Application\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
