{
  "name": "contributte/application",
  "description": "Extra contrib to nette/application",
  "keywords": [
    "nette",
    "application",
    "presenter",
    "component",
    "control"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/application",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/application": "^3.0.0 || ^4.0"
  },
  "require-dev": {
    "nette/http": "~2.4.8 || ^3.0.0",
    "ninjify/nunjuck": "^0.3.0",
    "ninjify/qa": "^0.13.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "psr/http-message": "~1.0.1",
    "tracy/tracy": "~2.9.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Application\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
